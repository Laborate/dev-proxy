{
    "general": {
        "port": 80,
        "accepted_repos": [
            "middleware"
        ],
        "domains": [
            "laborate.io",
            "laborate.me"
        ]
    },
    "forever": {
        "uid": "dev-proxy",
        "silent": false,
        "watch": true,
        "watch_directory": ".",
        "watch_ignore_dot": true,
        "watch_ignore_patterns": [
            "node_modules/**",
            ".*/**",
            ".*"
        ],
        "max_failures": 1000,
        "output_log": "../logs/dev-proxy/log.out",
        "error_log": "../logs/dev-proxy/log.error"
    }
}
